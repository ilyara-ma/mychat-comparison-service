{
  "name": "@moonactive/chat-comparison-tool",
  "version": "1.0.0",
  "description": "Microservice to validate Chat behavior against PubNub during migration",
  "main": "dist/app.js",
  "license": "ISC",
  "repository": "git://github.com/moonactive/chat-comparison-tool.git",
  "scripts": {
    "build": "rimraf lib && tsc",
    "start": "npm run build && node dist/app.js",
    "start:local": "AWS_PROFILE=moonactive-ci npm start",
    "test": "AWS_PROFILE=moonactive-ci nyc mocha 'test/**/*.test.ts' --exit --require ts-node/register --require @moonactive/moonactive-test-reporter --reporter mochawesome",
    "test:integration": "AWS_PROFILE=moonactive-ci mocha 'integration-test/**/*.test.ts' --reporter mochawesome --require ts-node/register --require @moonactive/moonactive-test-reporter --exit --timeout 200000 --retries 2",
    "lint": "node_modules/.bin/eslint . --ext .ts",
    "lint:fix": "node_modules/.bin/eslint . --ext .ts --fix",
    "husky": "husky install || true",
    "prepare": "npm run husky"
  },
  "lint-staged": {
    "*.ts": [
      "node_modules/.bin/eslint . --fix"
    ]
  },
  "dependencies": {
    "@moonactive/alerts": "8.7.0",
    "@moonactive/microservice-core": "6.5.9",
    "@moonactive/moonactive-feature-config": "7.6.4",
    "@moonactive/moonactive-logger": "6.14.1",
    "@moonactive/moonactive-routes": "4.18.1",
    "@moonactive/moonactive-machine-meta": "3.1.5",
    "@moonactive/moonactive-mysql-client": "^2.1.3",
    "@moonactive/moonactive-realtime-communications": "2.3.4-pr-56-b8cf4a0",
    "@moonactive/moonactive-secret-manager": "1.4.3",
    "@moonactive/moonactive-core-metrics": "1.2.1",
    "@moonactive/moonactive-server": "4.0.5",
    "@moonactive/request-context": "6.11.0",
    "@moonactive/teams-dal": "3.6.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@moonactive/eslint-config-ma-cm": "^2.1.1",
    "@moonactive/moonactive-test-reporter": "1.0.2",
    "@types/chai": "^4.3.11",
    "@types/lodash": "^4.14.202",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.6",
    "@types/proxyquire": "^1.3.31",
    "@types/sinon": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.2",
    "husky": "^8.0.3",
    "lint-staged": "^12.0.2",
    "mocha": "^9.2.2",
    "mochawesome": "7.0.1",
    "nyc": "^15.0.0",
    "proxyquire": "^2.1.3",
    "sinon": "17.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  }
}